gitchEffect = `<div class="c-glitch__img"></div><div class="c-glitch__img"></div><div class="c-glitch__img"></div><div class="c-glitch__img"></div><div class="c-glitch__img"></div>`; function addCss(e) { const t = document.createElement("style"); t.type = "text/css", t.innerHTML = e, document.head.appendChild(t) } function createGlitchEffect(e, t = {}) { t = { id: "glitch", cid: "f45712763fd72281b61dbe32e6e4e0772b69b211604519e941eef07b205037fai0", directContent: !1, ...t }; const { id: c, cid: n, directContent: d, css: i } = t; if (d ? loadCSS(`${bu}${n}`).then((() => { i && addCss(i) })) : fetchChildren(n).then((async ({ lci: e }) => { await loadCSS(`${bu}${e}`), i && addCss(i) })), i) { const e = document.createElement("style"); e.type = "text/css", e.innerHTML = i, document.head.appendChild(e) } const a = document.createElement("div"); a.innerHTML = gitchEffect, a.id = c, a.classList.add("c-glitch"), a.style.backgroundImage = `url(${e})`; return a.querySelectorAll(".c-glitch__img").forEach(((t, c) => { t.style.backgroundImage = `url(${e})` })), a }